baur=[]
baur2=[]
def superposicion():
  for x in range (1,3):
    name=input(f"{x}:Escriba un nombre para la lista 1:\n")
    baur.append(name)
    for x in range (0,1):
      name1=input("Escriba un nombre para la lista 2:\n")
      baur2.append(name1)
  if baur == baur2:
    print("True coincide un nombre en ambas lista.")
  else:
    print("False no hay nombres iguales en las 2 lista.")
definir=superposicion()
print(baur,baur2)
